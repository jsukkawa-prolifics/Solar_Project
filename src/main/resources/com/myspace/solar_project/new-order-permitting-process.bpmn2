      <bpmn2:script><![CDATA[System.out.println("Starting new-order-permitting process : "+kcontext.getProcessInstance().getId());

com.myspace.solar_project.NewOrder no = (com.myspace.solar_project.NewOrder) kcontext.getVariable("newOrder");
com.myspace.solar_project.Applicant app = (com.myspace.solar_project.Applicant) no.getApplicantDetails();
System.out.println("new-order-permitting process Order Type : "+no.getOrderType());

//Set Process Instance Id as Order Id
no.setOrderId(kcontext.getProcessInstance().getId());

//Set newOrder back to Process Variable
kcontext.setVariable("newOrder",no);

//Preparing request for HOA Approval Service
com.myspace.solar_project.HOAApprovalRequest hoaApprReq = new com.myspace.solar_project.HOAApprovalRequest();

hoaApprReq.setOrderType(newOrder.getOrderType());
hoaApprReq.setAddress(app.getAddress());
hoaApprReq.setPlotNumber(app.getPlot());

kcontext.setVariable("hoaServiceRequest",hoaApprReq);]]></bpmn2:script>
